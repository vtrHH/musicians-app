<section class="page-header">
   <h3>Albums found</h3>
</section>

<section class="offeritem-overview">
   {{#each albums}}
   <div> 
   <div class="card card-custom">
  <div class="card-header card-header-custom">{{this.title}}</div>
  {{#if images}}
  <img src="{{this.images.[2].url}}" alt="{{ this.name }}" class="card-img-top card-img-custom" alt="{{this.title}}">
  {{else}}
  <img src="/images/no-image-icon.png" class="card-img-top card-img-custom" alt="">
  {{/if}}
  <div class="card-body card-body-custom"> 
  <h4 class="card-text card-text-custom">{{this.name}}</h4>
  </div>
  <div class="card-link-overview">
    <a href="/music/tracks/{{ this.id }}" class="card-link card-link-custom">View Tracks</a>
  </div>
  </div>
   </div>
   {{/each}}
</section>